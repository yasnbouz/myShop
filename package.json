{
  "name": "typescript-nextjs-starter",
  "description": "A Shopify E-commerce starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "@yasnbouz"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "engines": {
    "node": ">=16.x",
    "pnpm": ">=7.x"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "type-check": "tsc --incremental false",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "graphql-types": "rimraf src/services/shopify/generated && graphql-codegen",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook",
    "e2e:test": "playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000/",
    "e2e:report": "playwright show-report",
    "analyze": "cross-env ANALYZE=true pnpm build"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "pnpm lint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.7.4",
    "@next/env": "12.3.1",
    "@tanstack/react-query": "4.14.5",
    "clsx": "1.2.1",
    "colord": "2.9.3",
    "next": "12.3.1",
    "next-seo": "5.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use-cart": "1.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@graphql-codegen/typescript-react-query": "4.0.5",
    "@next/bundle-analyzer": "13.0.2",
    "@playwright/test": "1.27.1",
    "@storybook/addon-a11y": "6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-viewport": "6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query-devtools": "4.14.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "babel-loader": "^8.2.5",
    "chromatic": "6.11.4",
    "commitizen": "4.2.5",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "graphql": "16.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "next-sitemap": "3.1.30",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sharp": "0.31.2",
    "storybook-addon-next-router": "4.0.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.8.4",
    "windicss": "3.5.6",
    "windicss-webpack-plugin": "1.7.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}